{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "vite --port 3000",
        "build": "tsc --noEmit && vite build",
        "serve": "vite preview",
        "build-env-check": "node ./react-build-env-checker.js && vite build",
        "test": "vitest",
        "test-all": "vitest --run",
        "test-all:coverage": "vitest --run --coverage src",
        "format": "prettier --write \"**/*.+(ts|tsx|js|scss|json|css|md)\"",
        "prepare": "cd ../ && husky install ./app/.husky",
        "cypress": "cypress open",
        "cypress-run": "cypress run --env grepTags=regression --browser chrome",
        "cypress-report-run": "cypress run --reporter mochawesome --env grepTags=regression",
        "cypress-report-merge": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesomemerged.json",
        "cypress-report-generate": "npx marge mochawesomemerged.json",
        "cypress-report": "rm -rf cypress/results/* && npm run cypress-report-run && npm run cypress-report-merge && npm run cypress-report-generate"
    },
    "dependencies": {
        "@cypress/grep": "^4.1.0",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.6.4",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "@vitejs/plugin-react-swc": "^3.11.0",
        "aws-rum-web": "^1.25.0",
        "aws-sdk": "^2.1692.0",
        "axios": "^1.11.0",
        "dotenv": "^17.2.1",
        "eslint-config-react-app": "^7.0.1",
        "fake-progress": "^1.0.4",
        "govuk-frontend": "^5.11.1",
        "history": "^5.3.0",
        "jwt-decode": "^4.0.0",
        "moment": "^2.30.1",
        "nhsuk-frontend": "^9.6.4",
        "nhsuk-react-components": "^5.0.0",
        "pdf-merger-js": "^5.1.2",
        "pdfjs-dist": "^5.4.54",
        "pdfjs-viewer-element": "^2.7.2",
        "pdfobject": "^2.3.1",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-hook-form": "^7.61.1",
        "react-router": "^7.7.1",
        "react-router-dom": "^7.7.1",
        "react-toggle": "^4.1.3",
        "sass": "^1.89.2",
        "serve": "^14.2.4",
        "sinon": "^21.0.0",
        "usehooks-ts": "^3.1.1",
        "util": "^0.12.5",
        "uuid": "^11.1.0",
        "validator": "^13.15.15",
        "vite": "^7.0.6",
        "vite-plugin-commonjs": "^0.10.4",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^4.3.0",
        "vite-tsconfig-paths": "^5.1.4",
        "web-vitals": "^5.0.3"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^24.1.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@types/react-toggle": "^4.0.5",
        "@types/sinon": "^17.0.4",
        "@types/uuid": "^10.0.0",
        "@types/validator": "^13.15.2",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "babel-plugin-named-exports-order": "^0.0.2",
        "cypress": "^14.5.3",
        "cypress-real-events": "^1.14.0",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-cypress": "^2.14.0",
        "eslint-plugin-prettier": "^5.5.1",
        "eslint-plugin-unused-imports": "^4.1.4",
        "happy-dom": "^18.0.1",
        "husky": "^9.1.7",
        "jest-axe": "^10.0.0",
        "jsdom": "^26.1.0",
        "lint-staged": "^16.1.2",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^5.0.0",
        "prettier": "^3.6.2",
        "prop-types": "^15.8.1",
        "typescript": "^5.8.3",
        "vitest": "^3.2.4",
        "webpack": "^5.100.2"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "vitest": {
        "moduleNameMapper": {
            "axios": "axios/dist/node/axios.cjs"
        }
    }
}
