{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "vite --port 3000",
        "build": "tsc --noEmit && vite build",
        "serve": "vite preview",
        "build-env-check": "node ./react-build-env-checker.js && vite build",
        "test": "vitest",
        "test-all": "vitest --run",
        "test-all:coverage": "vitest --run --coverage src",
        "format": "prettier --write \"**/*.+(ts|tsx|js|scss|json|css|md)\"",
        "prepare": "cd ../ && husky install ./app/.husky",
        "cypress": "cypress open",
        "cypress-run": "cypress run --env grepTags=regression --browser chrome",
        "cypress-report-run": "cypress run --reporter mochawesome --env grepTags=regression",
        "cypress-report-merge": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesomemerged.json",
        "cypress-report-generate": "npx marge mochawesomemerged.json",
        "cypress-report": "rm -rf cypress/results/* && npm run cypress-report-run && npm run cypress-report-merge && npm run cypress-report-generate",
        "prettier:check": "prettier . --config .prettierrc --exclude-files .prettierignore -c",
        "prettier:fix": "prettier . --config .prettierrc --exclude-files .prettierignore --write"
    },
    "dependencies": {
        "@vitejs/plugin-react-swc": "^4.0.1",
        "aws-rum-web": "^1.25.0",
        "axios": "^1.12.0",
        "dotenv": "^17.2.2",
        "eslint-config-react-app": "^7.0.1",
        "fake-progress": "^1.0.4",
        "govuk-frontend": "^5.11.1",
        "history": "^5.3.0",
        "jwt-decode": "^4.0.0",
        "moment": "^2.30.1",
        "nhsuk-frontend": "^9.6.4",
        "nhsuk-react-components": "^5.0.0",
        "pdf-merger-js": "^5.1.2",
        "pdfjs-dist": "^5.4.149",
        "pdfjs-viewer-element": "^2.7.2",
        "pdfobject": "^2.3.1",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.62.0",
        "react-router": "^7.8.2",
        "react-router-dom": "^7.8.2",
        "react-toggle": "^4.1.3",
        "sass": "^1.92.1",
        "serve": "^14.2.5",
        "sinon": "^21.0.0",
        "usehooks-ts": "^3.1.1",
        "util": "^0.12.5",
        "uuid": "^11.1.0",
        "validator": "^13.15.15",
        "vite": "^7.1.5",
        "vite-plugin-commonjs": "^0.10.4",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^4.5.0",
        "vite-tsconfig-paths": "^5.1.4",
        "web-vitals": "^5.1.0"
    },
    "devDependencies": {
        "@aws-sdk/client-dynamodb": "^3.887.0",
        "@aws-sdk/client-s3": "^3.887.0",
        "@aws-sdk/credential-providers": "^3.887.0",
        "@aws-sdk/util-dynamodb": "^3.887.0",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@cypress/grep": "^4.1.1",
        "@testing-library/dom": "10.4.1",
        "@testing-library/jest-dom": "6.8.0",
        "@testing-library/react": "16.3.0",
        "@testing-library/user-event": "14.6.1",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^24.3.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@types/react-toggle": "^4.0.5",
        "@types/sinon": "^17.0.4",
        "@types/uuid": "^10.0.0",
        "@types/validator": "^13.15.3",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "babel-plugin-named-exports-order": "^0.0.2",
        "cypress": "^14.5.4",
        "cypress-real-events": "^1.15.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-cypress": "^3.6.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-unused-imports": "^4.2.0",
        "happy-dom": "^18.0.1",
        "husky": "^9.1.7",
        "jest-axe": "^10.0.0",
        "jsdom": "^26.1.0",
        "lint-staged": "^16.1.6",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^5.0.0",
        "prettier": "^3.6.2",
        "prop-types": "^15.8.1",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "vitest": {
        "moduleNameMapper": {
            "axios": "axios/dist/node/axios.cjs"
        }
    }
}
